{
  "ReRoutes": [
    {
      // 下游（转发的服务器地址模板）
      "DownstreamPathTemplate": "/{url}",
      "DownstreamScheme": "http",
      // 下游服务器地址
      "DownstreamHostAndPorts": [
        {
          "Host": "127.0.0.1",
          "Port": "5010"
        },
        {
          "Host": "127.0.0.1",
          "Port": "5011"
        }
      ],
      // 上游（请求路径模板）
      "UpstreamPathTemplate": "/api/{url}",
      "UpstreamHttpMethod": [ "Get", "Post", "DELETE", "PUT" ],
      /**
      LeastConnection – 将请求发往空闲的那个服务器
RoundRobin – 轮流发送
NoLoadBalance – 总是发往个请求或者是服务发现
*/
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ]
}
