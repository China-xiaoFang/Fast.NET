<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<PackageOutputPath>$(SolutionDir)..\..\nupkgs</PackageOutputPath>
		<Version>3.0.4</Version>
		<PackageVersion>$(Version)</PackageVersion>
		<Authors>1.8K仔</Authors>
		<Description>一个快速构建项目的 .NET 框架。</Description>
		<PackageTags>Fast;ASP.NET;dotNET China;Cross Platform;1.8K仔</PackageTags>
		<Product>Fast</Product>
		<Copyright>版权所有 © 2018-2023 1.8K仔</Copyright>
		<PackageProjectUrl>https://18kboy.icu</PackageProjectUrl>
		<PackageIcon>Fast.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://gitee.com/Net-18K/fast.net</RepositoryUrl>
		<RepositoryType>Gitee</RepositoryType>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<DocumentationFile>$(MSBuildProjectName).xml</DocumentationFile>
		<GenerateResxSourceIncludeDefaultValues>true</GenerateResxSourceIncludeDefaultValues>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<NoWarn>1701;1702;1591</NoWarn>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<NoWarn>1701;1702;1591</NoWarn>
	</PropertyGroup>

	<ItemGroup>
    <Compile Remove="logs\**" />
    <Content Remove="logs\**" />
    <EmbeddedResource Remove="logs\**" />
    <None Remove="logs\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="RabbitMQ.Client" Version="6.5.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Fast.NET\Authentication\Fast.Authentication\Fast.Authentication.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Core\Fast.Core\Fast.Core.csproj" />
    <ProjectReference Include="..\..\Fast.NET\CorsAccessor\Fast.CorsAccessor\Fast.CorsAccessor.csproj" />
    <ProjectReference Include="..\..\Fast.NET\DataValidation\Fast.DataValidation\Fast.DataValidation.csproj" />
    <ProjectReference Include="..\..\Fast.NET\DynamicApplication\Fast.DynamicApplication\Fast.DynamicApplication.csproj" />
    <ProjectReference Include="..\..\Fast.NET\EventBus\Fast.EventBus\Fast.EventBus.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Exception\Fast.Exception\Fast.Exception.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Logging\Fast.Logging\Fast.Logging.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Mapster\Fast.Mapster\Fast.Mapster.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Serialization\Fast.Serialization\Fast.Serialization.csproj" />
    <ProjectReference Include="..\..\Fast.NET\SpecificationDocument\Fast.SpecificationDocument\Fast.SpecificationDocument.csproj" />
    <ProjectReference Include="..\..\Fast.NET\Sugar\Fast.Sugar\Fast.Sugar.csproj" />
    <ProjectReference Include="..\..\Fast.NET\UnifyResult\Fast.UnifyResult\Fast.UnifyResult.csproj" />
  </ItemGroup>

</Project>
